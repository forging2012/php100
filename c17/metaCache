<script type="text/javascript">
if(typeof(Event) != "undefined") {
        var aa;
        aa = new AutoComplete(document.sb,"query");
        aa.setSearchServer("http://www.youdao.com/search");
        aa.setSuggestServer("http://www.youdao.com/");
        if(Event.observe) {
                Event.observe(window, "load", function() {
	if(isIE6())
		onLoadHandler();
        focusFlag = true;
	if(isIE7() || isIE8() || isIE6())
           setTimeout(function(){
        	setfocus();
		var elem  = document.getElementById("query");
		if(elem.value == yodaoDeafult)
			elem.value="";
	   }, 10);
        });
    }
}
var searchBoxUserKeyed = false;

if(!isIE6())
       onLoadHandler();
function setfocus(){
   var input = document.getElementById('query');
   input.focus();
    var flag = isIE6();
    if(flag){
        var tagName = document.activeElement.tagName;
	var id = document.activeElement.id;
	if(tagName == "IFRAME" || tagName == "BODY" ||(tagName == "INPUT" && id == "query")){
   	    if(!isFocusChange())
		input.focus();
	}
	if(input.value==yodaoDefault){
		input.value="";
	}
    }
   focusFlag = true;
   Event.observe(input, "keydown", autoBlurSearchBox);
 }

function isIE6(){
   var notMSIE=(navigator&&navigator.userAgent.toLowerCase().indexOf("msie")==-1);
   if(!notMSIE){
	//if(navigator.appVersion.indexOf("MSIE 6") != -1)
		return true;
    }
   return false;
}
function isIE7(){
   var notMSIE=(navigator&&navigator.userAgent.toLowerCase().indexOf("msie")==-1);
   if(!notMSIE){
	if(navigator.appVersion.indexOf("MSIE 7") != -1)
		return true;
    }
   return false;
}

function isIE8(){
   var notMSIE=(navigator&&navigator.userAgent.toLowerCase().indexOf("msie")==-1);
   if(!notMSIE){
	if(navigator.appVersion.indexOf("MSIE 8") != -1)
		return true;
    }
   return false;
}

function isFF(){
	return navigator.userAgent.indexOf("Firefox")>0;
}
function isFocusChange(){
   var input = document.getElementById("userName");
   if(input && input.value != "")
	return true;
   return false;
}
function autoBlurSearchBox(e) {
  var navkeys = new Array(32, 33, 34, 35, 36, 37, 38, 39, 40);
  if(!searchBoxUserKeyed) {
    if(navkeys.indexOf(e.keyCode) != -1) {
        var input = document.getElementById('query');
        input.blur();
    }else{
       var input = document.getElementById('query');
       input.focus();
    }
  }
  searchBoxUserKeyed = true;
}
var tipFlag = true;
var YodaoCurrentTab = document.getElementById("yodaoWeb");
function changeProduct(o){
	o = o.parentNode;
       var BtnName = "网页搜索";
	var SoBtn = document.getElementById("yodaoBtn");
	var SoForm = document.getElementById("yodaoForm");
        if(o == YodaoCurrentTab){
                return false;
        }else {
                switch(o.id){
                        case "yodaoWeb":
                                BtnName = "网页搜索";
                                act = "http://www.youdao.com/search";
                                sserver = "http://www.youdao.com";
                                break;
                        case "yodaoImage":
				tipFlag = false;
				document.getElementById("imageLink").href = "javascript:void(0)";
                                act = "http://image.youdao.com/search";
                                sserver = "http://image.youdao.com";
                                break;
                        case "yodaoNews":
				tipFlag = false;
				document.getElementById("newsLink").href = "javascript:void(0)";
                                act = "http://news.youdao.com/search";
                                sserver = "http://news.youdao.com";
                                break;
                        case "yodaoBlog":
				tipFlag = false;
				document.getElementById("blogLink").href = "javascript:void(0)";
                                act = "http://blog.youdao.com/search";
                                sserver = "http://blog.youdao.com";
                                break;
                        case "yodaoDict":
				tipFlag = false;
				document.getElementById("dictLink").href = "javascript:void(0)";
                                act = "http://dict.youdao.com/search";
                                sserver = "http://dict.youdao.com";
                                break;
                        case "yodaoMusic":
				tipFlag = false;
				document.getElementById("mp3Link").href = "javascript:void(0)";
                                act = "http://mp3.youdao.com/search";
                                sserver = "http://mp3.youdao.com";
                                break;

                }
                var url;
		var input = document.getElementById('query');
		var q;
		if(input.value == "搜索超过一百亿中文网页")
		  q="";
                else{
                	q=input.value;
			q=q.replace(/(^link:)|(^inlink:)|(^related:)/,""); 
			q=encodeURIComponent(q);
                }
		if(q == ""){
		    url = sserver + "?keyfrom=163index";
		}else
		   url = act + "?keyfrom=163index&q=" + q;
		window.open(url, "_parent");
                document.getElementById("query").focus();
                return false;
        }
}
//if(!isIE6())
    document.getElementById("query").focus();
</script>
<script type="text/javascript">
   function inputBlur(e){
    focusFlag = false;
    if (e.value=='') {
	e.style.color="#908f8f";
	e.style.height="19px";
	e.style.paddingTop="5px";
	e.style.font="13px '宋体',arial";
       setTimeout(function(){
		if(tipFlag == true && focusFlag == false)
	 	   e.value=yodaoDefault;
	}, 125);
   }else{
  }   
 }
 

   
</script>
