<script type="text/javascript">
function getWeather4Youdao(xmlDom)
{
	//alert("test2");
        var contemt="";
	var weatherDiv=document.getElementById("youdao_weather");
	var node=xmlDom.getElementsByTagName("weather");
    if(node.length){
	  contemt+="<CENTER>";
	  var len=node.length;
	  for(var i=0;i<len ;i++)
	  {
	  	var c=node[i].getAttribute("c");
	  	var city=node[i].getAttribute("city");
	  	var wd=node[i].getAttribute("wd");
	  	var qx=node[i].getAttribute("qx");
	  	var img=node[i].getAttribute("qximg");
	  	var imgs=img.split(",");
                contemt+="<table cellpadding=0 cellspacing=0 border=0 style=\"margin-top:12px\" >";
	        contemt+="<tr><td style=\"text-align:left;margin:auto;cursor:pointer;color:#1f3a87;line-height:130%\">"
		contemt+="<center>" + c + " 今天"+"</center>";
		contemt+="</td></tr>";
		if(img!=''&& img.length>0){
		   if(imgs.length > 0){
			contemt+="<tr><td style=\"text-align:left;margin:auto;cursor:pointer;color:#1f3a87;line-height:220% \">";
			contemt+="<center><div class=\"weather_icon\" style=\"margin:2px 3px 2px 0px\">";
	  		contemt+='<img width=25 height=25 src=http://img1.cache.netease.com/cnews/img/weatherlogo/'+imgs[0]+'>';
	  		contemt+="</div>";
	  		contemt+=qx;
			contemt+="</center></td></tr>";
		   }
		}
		contemt+="<tr><td style=\"text-align:left;margin:auto;cursor:pointer;color:#1f3a87;line-height:140% \"><center>";
		contemt+=wd;
		contemt+="</center></td></tr>";
		contemt+="</table>";
		weatherDiv.onclick= function(){
                    top.location.href="http://www.youdao.com/search?keyfrom=163index.weather&q=" + c + "天气"; 
		};
		break;
	  }
	
	  contemt+="</CENTER>";
     }
	  weatherDiv.innerHTML=contemt;		
}

function getTimeSuffix(){
   return "timeflag="+new Date();
}
function weather4Youdao(coder)
{
	loadXML("GET","/weatherxml/"+coder+".xml",getWeather4Youdao);
}
weather4Youdao(wloc);

</script>
